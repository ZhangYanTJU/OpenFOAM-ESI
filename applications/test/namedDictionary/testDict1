FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      dictionary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

entry1
(
    value1
    value2 ;  // spurious trailing ';' is removed
    this { correct true; }

    { }  // Empty everything == ignore

    { anonymous true; }
);


actions1
(
    starting
    {
        name    self;
        type    faceSet;
        action  new;
        source  something;
    }

    {
        name    self;
        type    faceSet;
        action  subset;
        source  something;
    }
);


