FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    maxY
    {
        type            inletOutlet;
        phi             phi.gas;
        inletValue      uniform 1;
        value           uniform 0;
    }

    symmetry
    {
        type            symmetry;
    }

    maxX
    {
        type            zeroGradient;
    }

    minY
    {
        type            zeroGradient;
    }

    empty
    {
        type            empty;
    }

    water_to_solid
    {
        type            zeroGradient;
    }
}


