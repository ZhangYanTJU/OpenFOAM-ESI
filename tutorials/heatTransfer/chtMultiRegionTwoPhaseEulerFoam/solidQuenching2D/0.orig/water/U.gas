FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U.gas;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    maxY
    {
        type            pressureInletOutletVelocity;
        phi             phi.gas;
        value           uniform ( 0 0 0 );
        inletValue      uniform ( 0 0 0 );
    }

    maxX
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }

    minY
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }

    water_to_solid
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
}


