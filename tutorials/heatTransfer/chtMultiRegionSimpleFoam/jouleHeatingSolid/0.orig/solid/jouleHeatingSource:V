FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      jouleHeatingSource:V;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 2 -3 0 0 -1 0 ];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 1.5;
    }

    right
    {
        type            fixedValue;
        value           uniform 0;
    }

    top
    {
        type            zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}


