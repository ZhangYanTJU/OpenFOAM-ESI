FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform 0.1;

boundaryField
{
    "(maxY|minX|maxX|minY|maxZ)"
    {
        type            inletOutlet;
        value           uniform 0.1;
        inletValue      uniform 0.1;
    }

    "(air_to_floor|air_to_solid)"
    {
        type            kqRWallFunction;
        value           uniform 0.1;
        inletValue      uniform 0.1;
    }
}


