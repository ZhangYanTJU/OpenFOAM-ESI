FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  3;

// method          assembleDecomp;
// method          hierarchical;
// method          simple;
// method          manual;
method  scotch;

constraints
{
    faces
    {
        type    preserveFaceZones;
        zones   (f1 f2 f3 f4 f5);
        enabled true;
    }
}

