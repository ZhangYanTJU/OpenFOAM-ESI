FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    symmetry
    {
        type            symmetryPlane;
    }

    walls
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }

    ice_to_cabin
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }

    ice_to_exterior
    {
        type            fixedFluxPressure;
        value           uniform 100000;
    }
}


