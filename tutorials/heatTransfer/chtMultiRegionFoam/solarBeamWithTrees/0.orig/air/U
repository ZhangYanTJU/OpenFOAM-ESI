FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0.1 0 0 );

boundaryField
{
    "(maxY|minX|maxX|minY|maxZ)"
    {
        type            pressureInletOutletVelocity;
        value           uniform ( 0 0 0 );
        phi             phi;
    }

    "(air_to_floor|air_to_solid)"
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
        phi             phi;
    }

    region0_to_floor_floor_zone
    {
        type            fixedValue;
        value           uniform ( 0 0 0 );
    }
}


