FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.00015;

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue    table
        (
            (0   0.00015)
            (60  0.00015)
            (61  0.00015)
            (100 0.00015)
        );
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.00015;
        value           uniform 0.00015;
    }

    symmetry
    {
        type            symmetryPlane;
    }

    walls
    {
        type            kqRWallFunction;
        value           uniform 0.00015;
    }

    cabin_to_windshield
    {
        type            kqRWallFunction;
        value           uniform 0.00015;
    }
}


// ************************************************************************* //
