FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitTAir
{
    type       limitTemperature;
    min        280;
    max        310;
    selectionMode all;
    active      yes;
}

dampVelocity
{
    type        velocityDampingConstraint;
    active      yes;

    velocityDampingConstraintCoeffs
    {
        selectionMode all;
        UMax 10;
    }
}


