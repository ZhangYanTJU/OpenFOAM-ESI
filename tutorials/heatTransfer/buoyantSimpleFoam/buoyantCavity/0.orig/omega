FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.12;

boundaryField
{
    "(frontAndBack|topAndBottom|hot|cold)"
    {
        type            omegaWallFunction;
        value           uniform 0.12;
    }
}


