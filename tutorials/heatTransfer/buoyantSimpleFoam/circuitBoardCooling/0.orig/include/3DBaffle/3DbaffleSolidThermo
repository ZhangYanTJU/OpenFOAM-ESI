thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIso;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight       20;
    }
    transport
    {
        kappa           ${Kappa};
    }
    thermodynamics
    {
        Hf              0;
        Cp              ${Cp};
    }
    equationOfState
    {
        rho             ${rho};
    }
}

radiation
{
    radiationModel          opaqueSolid;
    absorptionEmissionModel none;
    scatterModel            none;
    transmissivityModel     none;
}


