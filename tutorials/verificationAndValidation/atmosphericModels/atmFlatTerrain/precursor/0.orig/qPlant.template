FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      qPlant;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField  uniform  Q_PLANT;

boundaryField
{
    "(bottom|top)"
    {
        type            fixedValue;
        value           uniform 0;
    }

    "(inlet|outlet|left|right)"
    {
        type            cyclic;
    }
}


