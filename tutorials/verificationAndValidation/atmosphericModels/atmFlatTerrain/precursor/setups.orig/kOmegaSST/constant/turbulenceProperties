FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType      RAS;

RAS
{
    RASModel        kOmegaSST;

    turbulence      on;

    printCoeffs     on;

    kOmegaSSTCoeffs
    {
        betaStar        0.09;
        sigmaK          1;
        C1              1.44;
        C2              1.92;
    }
}


