FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      leafAreaDensity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    "(bottom|top)"
    {
        type            fixedValue;
        value           uniform 0;
    }

    "(inlet|outlet|left|right)"
    {
        type            cyclic;
    }
}


