FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      Uf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    coupledWall_top                 // Patch to Region0
    {
        type            slip;
    }

    region0_to_filmRegion_coupledWall // Patch to pyrolysis
    {
        type            noSlip;
    }

    outlet
    {
        type zeroGradient;
    }

    "side.*"
    {
        type            noSlip;
    }

    inlet
    {
        type            fixedValue;
        rho             rhof;
        value           uniform (0 0 -0.01);
    }
}


