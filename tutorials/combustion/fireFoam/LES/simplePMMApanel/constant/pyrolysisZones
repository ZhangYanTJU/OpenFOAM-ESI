FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      pyrolysisZones;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pyrolysis
{
    active          true;

    pyrolysisModel  reactingOneDim;

    region          panelRegion;

    reactingOneDimCoeffs
    {
        qrHSource       no; //Energy source term due in depht radiation

        filmCoupled     false;

        qrHSource       on;

        minimumDelta    1e-6;

        moveMesh        true;

        useChemistrySolvers false;
    }

    infoOutput      true;
}


