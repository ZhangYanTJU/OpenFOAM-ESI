FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            symmetryPlane;
    }

    right
    {
        type            symmetryPlane;
    }

    down
    {
        type            symmetryPlane;
    }

    up
    {
        type            fixedValue;
        value           uniform 0;
    }

    hole
    {
        type            fixedValue;
        value           uniform 45000;
    }

    defaultFaces
    {
        type            empty;
    }
}


