FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    "(HULL_DISK|HULL|BLADES|HUB_DISK|HUB)"
    {
        type            calculated;
        value           uniform (0 0 0);
    }

    oversetPatch
    {
        patchType       overset;
        type            zeroGradient;
    }

    overset-1
    {
        patchType       overset;
        type            zeroGradient;
    }
}


