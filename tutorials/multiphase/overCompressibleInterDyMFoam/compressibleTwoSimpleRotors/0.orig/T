FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(hole|outlet|inlet)"
    {
        type            fixedValue;
        value           $internalField;
    }

    walls
    {
        type            fixedValue;
        value           uniform 400;
    }

    overset
    {
        type            overset;
    }
}


