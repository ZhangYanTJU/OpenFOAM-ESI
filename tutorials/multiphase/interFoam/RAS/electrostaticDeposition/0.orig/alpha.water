FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    metalSheet
    {
        type            zeroGradient;
    }

    "(side-01|side-06|side-03|side-04|side-05)"
    {
        type            variableHeightFlowRate;
        lowerBound      0;
        upperBound      1;
        value           uniform 0;
    }

    side-02
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
}


