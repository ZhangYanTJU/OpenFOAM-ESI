FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      extrudeProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

constructFrom   mesh;
sourceCase      "<case>";
sourcePatches   (outlet);

flipNormals     false;

extrudeModel    linearDirection;

nLayers             40;
expansionRatio      1.0;

linearDirectionCoeffs
{
    direction       (1 -0.2 -0.03);
    thickness       40;
}

mergeFaces      false;


