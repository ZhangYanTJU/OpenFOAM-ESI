FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      pa;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontBack
    {
        type            empty;
    }

    designWall  
    {
        type            zeroGradient;
    }

    topOPatch
    {
        type            zeroGradient;
    }

    IEntranceWall  
    {
        type            zeroGradient;
    }

    OEntranceWall  
    {
        type            zeroGradient;
    }

    OREntranceWall  
    {
        type            zeroGradient;
    }

    inlet  
    {
        type            zeroGradient;
    }

    outlet
    {
        type            adjointOutletPressure;
        value           uniform 0.0;
    }
    
    outlet-right
    {
        type            adjointOutletPressure;
        value           uniform 0.0;
    }
}

