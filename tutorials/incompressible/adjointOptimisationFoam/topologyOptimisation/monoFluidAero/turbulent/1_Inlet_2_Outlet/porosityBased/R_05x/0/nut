FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.e-4;

boundaryField
{
    frontBack
    {
        type            empty;
    }

    designWall 
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }

    IEntranceWall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }

    OEntranceWall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }

    OREntranceWall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }

    inlet  
    {
        type            fixedValue;
        value           uniform 1.e-4;
    }

    outlet
    {
        type            zeroGradient;
    }

    outlet-right
    {
        type            zeroGradient;
    }
}

