{
    nNonOrthogonalCorrectors 0;
}

solvers
{
    "p|pa.*|bTilda"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-09;
        relTol          0.1;
    }

    "U|Ua.*"
    {
        solver      PBiCGStab;
        preconditioner   DILU;
        tolerance   1e-9;
        relTol      0.1;
    };
}

relaxationFactors 
{
    fields
    {
        p               0.3;
       "pa.*"           0.2;
    }
    equations
    {
        U               0.7;
       "Ua.*"           0.5;
    }
