FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    frontBack
    {
        type            empty;
    }

    designWall  
    {
        type            adjointWallVelocity;
        value           uniform (0 0 0);
    }

    IEntranceWall  
    {
        type            adjointWallVelocity;
        value           uniform (0 0 0);
    }

    OEntranceWall  
    {
        type            adjointWallVelocity;
        value           uniform (0 0 0);
    }

    OREntranceWall  
    {
        type            adjointWallVelocity;
        value           uniform (0 0 0);
    }

    inlet  
    {
        type            adjointInletVelocity;
        value           uniform (0 0 0);
    }

    outlet
    {
        type            adjointOutletVelocity;
        value           uniform (0 0 0);
    }
    
    outlet-right
    {
        type            adjointOutletVelocity;
        value           uniform (0 0 0);
    }

}

