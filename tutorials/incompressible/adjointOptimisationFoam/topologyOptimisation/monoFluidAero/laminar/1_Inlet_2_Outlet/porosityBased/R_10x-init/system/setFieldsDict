FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha 0
);

regions
(
    boxToCell
    {
        box (0.0 0.0 0.0)(0.35 0.15 1.0);

        fieldValues
        (
            volScalarFieldValue alpha 1
        );
    }
);

