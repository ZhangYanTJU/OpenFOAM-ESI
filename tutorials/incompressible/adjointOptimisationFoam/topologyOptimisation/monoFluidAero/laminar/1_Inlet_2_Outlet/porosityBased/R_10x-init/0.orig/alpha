FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      alpha;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    designWall
    {
        type            zeroGradient;
    }
    IEntranceWall
    {
        type            zeroGradient;
    }
    OEntranceWall
    {
        type            zeroGradient;
    }
    OREntranceWall
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet-right
    {
        type            fixedValue;
        value           uniform 0;
    }
    frontBack
    {
        type            empty;
    }
}


