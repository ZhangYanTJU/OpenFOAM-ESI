FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes     
{
    default         Gauss linear;
}

divSchemes      
{
    default                              Gauss linear;
    div(phi,U)                           bounded Gauss upwind;
    div(-phi,Ua)                         bounded Gauss upwind;
}

interpolationSchemes 
{
    default             linear;
}
laplacianSchemes 
{
    default         Gauss linear corrected;
}
snGradSchemes   
{
    default         corrected;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
