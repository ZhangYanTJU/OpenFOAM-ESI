actions
(

    {
        name    fixedWalls;
        type    faceSet;
        action  new;
        source  patchToFace;
        patch   fixedWalls;
    }

    {
        name    inlet;
        type    faceSet;
        action  new;
        source  faceToFace;
        set     fixedWalls;
    }

    {
        name    inlet;
        type    faceSet;
        action  subset;
        source  boxToFace;
        sourceInfo
        {
            box  (0.030 0.130 0.080) (0.070 0.170 0.120);
        }
    }
    {
        name    inletZone;
        type    cellSet;
        action  new;
        source  faceToCell;
        set     inlet;
        option  any;
    }

    {
        name    outlet1;
        type    faceSet;
        action  new;
        source  faceToFace;
        set     fixedWalls;
    }

    {
        name    outlet1;
