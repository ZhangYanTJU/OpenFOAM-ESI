FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "wall|fixedWalls|topOPatch"  
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    inlet  
    {
        type            adjointInletVelocity;
        value           uniform (0 0 0);
    }
    "outlet.*"
    {
        type            adjointOutletVelocityFlux;
        value           uniform (0 0 0);
    }
    symmetry
    {
        type            symmetry;
    }
    #includeEtc "caseDicts/setConstraintTypes"
}

