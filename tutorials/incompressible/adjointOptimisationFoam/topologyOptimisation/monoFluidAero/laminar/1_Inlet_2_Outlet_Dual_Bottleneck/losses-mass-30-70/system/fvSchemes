FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes     
{
    default         Gauss linear;
    gradUConv       cellLimited Gauss linear 1;
}

divSchemes      
{
    default            Gauss linear;
    div(phi,U)         bounded Gauss linearUpwind gradUConv;
   "div\(-phi,Ua.*\)"  bounded Gauss linearUpwind gradUConv;
}

interpolationSchemes 
{
    default         linear;
}

laplacianSchemes 
{
    default         Gauss linear corrected;
}

snGradSchemes   
{
    default         corrected;
}


