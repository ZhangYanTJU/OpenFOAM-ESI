FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      dynamicMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solver laplacianMotionSolver;

laplacianMotionSolverCoeffs
{
    diffusivity uniform;
    iters       1000;
    tolerance   1.e-06;
}


