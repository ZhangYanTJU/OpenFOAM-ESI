FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      Ua;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    frontBack
    {
        type            empty;
    }

    sideWall
    {
        type            adjointWallVelocity;
        value           uniform ( 0 0 0 );
    }

    upper
    {
        type            adjointWallVelocity;
        value           uniform ( 0 0 0 );
    }

    Inlet
    {
        type            adjointInletVelocity;
        value           uniform ( 0 0 0 );
    }

    Outlet
    {
        type            adjointOutletVelocity;
        value           uniform ( 0 0 0 );
    }

    lower
    {
        type            adjointWallVelocity;
        value           uniform ( 0 0 0 );
    }
}


// ************************************************************************* //
