FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            sets;
libs            (sampling);
writeControl    onEnd;
interpolationScheme cellPoint;
setFormat       raw;

fields          ( U UMean );

sets
{
    xm10
    {
        type            face;
        axis            z;
        start           (2.5 4.5 0);
        end             (2.5 4.5 2);
    }
