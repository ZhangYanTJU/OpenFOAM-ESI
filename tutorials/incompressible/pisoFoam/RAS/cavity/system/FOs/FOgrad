grad1
{
    // Mandatory entries
    type            grad;
    libs            (fieldFunctionObjects);
    field           U;

    // Optional (inherited) entries
    result          gradU;
    region          region0;
    enabled         true;
    log             true;
    timeStart       0.25;
    timeEnd         1000;
    executeControl  timeStep;
    executeInterval 5;
    writeControl    timeStep;
    writeInterval   25;
}


