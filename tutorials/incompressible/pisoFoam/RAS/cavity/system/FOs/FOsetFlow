setFlow1
{
    // Mandatory entries
    type            setFlow;
    libs            (fieldFunctionObjects);
    mode            function;
    scale           1;
    reverseTime     1;
    velocity        (1 0 0);

    // Optional (inherited) entries
    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         1000;
    executeControl  timeStep;
    executeInterval 500;
    writeControl    timeStep;
    writeInterval   500;
}


