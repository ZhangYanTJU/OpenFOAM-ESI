FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.05;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.05;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.05;
        value           uniform 0.05;
    }

    lowerWall
    {
        type            fixedValue;
        value           uniform 0;
    }

    "motorBike_.*"
    {
        type            fixedValue;
        value           uniform 0;
    }

    upperWall
    {
        type            symmetryPlane;
    }

    "(front|back)"
    {
        type            symmetryPlane;
    }

    "proc.*"
    {
        type            processor;
    }
}


