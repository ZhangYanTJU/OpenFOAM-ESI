FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    front
    {
        type            noSlip;
    }

    back
    {
        type            noSlip;
    }

    walls
    {
        type            noSlip;
    }

    porosityWall
    {
        type            slip;
        value           $internalField;
    }

    inlet
    {
        type            flowRateInletVelocity;
        massFlowRate    constant 0.1;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}


// ************************************************************************* //
