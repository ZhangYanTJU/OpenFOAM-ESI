FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      tracer0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet_air
    {
        type            fixedValue;
        value           uniform 0;
    }

    inlet_fuel
    {
        type            fixedValue;
        value           uniform 1;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }

    ".*"
    {
        type            zeroGradient;
    }
}


