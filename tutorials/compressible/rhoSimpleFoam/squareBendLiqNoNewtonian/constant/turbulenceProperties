FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  laminar;

laminar
{
    model       generalizedNewtonian;

    viscosityModel  powerLaw;
    nuMin           1e-03;
    nuMax           1;
    k               0.02;
    n               0.4;
}


