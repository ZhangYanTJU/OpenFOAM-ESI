FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      faOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pressure
{
    type            externalFileSource;
    fieldName       ws_vibrationShell;
    tableName       p;
    active          true;
    timeStart       0.001;
    duration        0.03;
    region          vibrationShell;
    selectionMode   all;

    sampleFormat    ensight;
    sampleFile      "<constant>/sampled/window/windowPatch.case";
    filterRadius    3e-3;
    filterSweeps    10;
}


