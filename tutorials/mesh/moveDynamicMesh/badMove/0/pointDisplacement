FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    minX
    {
        type    slip;
    }
    maxX
    {
        $minX
    }
    minY
    {
        $minX
    }
    maxY
    {
        $minX
    }
    minZ
    {
        $minX
    }
    maxZ
    {
        $minX
    }
    boundaryEdges
    {
        type            uniformFixedValue;
        uniformValue    table
        (
            ( 0.0        (0 0 0))
            ( 1.0        (1.0 -1.0 0))
            //(10.0        (0 0 0))
        );
    }
}


// ************************************************************************* //
