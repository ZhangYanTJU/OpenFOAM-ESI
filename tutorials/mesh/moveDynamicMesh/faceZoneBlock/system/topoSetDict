// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name    c0;
        type    cellSet;
        action  new;
        source  boxToCell;
        box     (-1 -1 -1)(1 1 1);
    }
    {
        name    movingZoneFaces;
        type    faceSet;
        action  new;
        source  cellToFace;
        set     c0;
        option  outside;
    }
    {
        name    movingZone;
        type    faceZoneSet;
        action  new;
        source  setToFaceZone;
        faceSet movingZoneFaces;
    }
);

