FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          fvSchemes;
}

ddtSchemes
{
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

wallDist
{
}

geometry
{
    type            highAspectRatio;
    minAspect       10;
    maxAspect       100;
}


