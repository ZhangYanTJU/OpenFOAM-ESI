FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name    c0;
        type    cellSet;
        action  new;
        source  boundaryToCell;
    }
    {
        name    c0;
        type    cellSet;
        action  subset;
        source  boxToCell;
        sourceInfo
        {
            box (-100 -100 -100) (0 100 100);
        }
    }
    {
        name    c0;
        type    cellSet;
        action  add;
        source  boxToCell;
        sourceInfo
        {
            box (-0.3 -0.3 -0.3) (0 0 0);
        }
    }
);

