// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

T
{
    boundaryField
    {
        inlet
        {
            type    uniformFixedValue;

            uniformValue
            {
                type    table;

                values
                (
                    (0 1e-12)
                    (0.1 1e-12)
                    (0.10001 20)
                );
            }
        }
    }
}


