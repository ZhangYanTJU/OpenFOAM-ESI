FoamFile
{
    version     2.0;
    format      ascii;
    class       areaScalarField;
    object      manningField;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 1 0 0 0 0 0];

internalField       uniform 0.1;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type        zeroGradient;
    }

    outlet
    {
        type        zeroGradient;
    }

    side
    {
        type        zeroGradient;
    }

    symmetry
    {
        type        symmetry;
    }

    cylinder
    {
        type        zeroGradient;
    }
}


