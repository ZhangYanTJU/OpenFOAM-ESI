FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      iDof;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    flow
    {
        type            zeroGradient;
    }

    obstacle
    {
        type            calculated;
        value           uniform 0;
    }

    frontAndBack
    {
        type            empty;
    }
}


