FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      boundaryU;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    "(outlet|inlet|sides)"
    {
        type            fixedValue;
        value           uniform (1325 -352 823);
    }
}


